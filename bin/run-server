#! /usr/bin/env python
import _mypath
from pony_build import server, simple_app

HOST='localhost'
PORT=8080

app = simple_app.SimpleApp()

server = server.create(HOST, PORT, app)

try:
    print 'serving on http://%s:%d/xmlrpc' % (HOST, PORT)
    server.serve_forever()
except KeyboardInterrupt:
    print 'exiting'
